<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

<changeSet id="5-drop-foreign-keys" author="StudentPP1" runAlways="true">
    <dropForeignKeyConstraint baseTableName="user_read_stories" constraintName="fk_user_read_user"/>
    <dropForeignKeyConstraint baseTableName="user_liked_stories" constraintName="fk_user_liked_user"/>
</changeSet>
<changeSet id="6-drop-user-read-stories" author="StudentPP1" runAlways="true">
<dropTable tableName="user_read_stories"/>
</changeSet>

<changeSet id="7-drop-user-liked-stories" author="StudentPP1" runAlways="true">
<dropTable tableName="user_liked_stories"/>
</changeSet>
<changeSet id="8-drop-app-user" author="StudentPP1" runAlways="true">
<dropTable tableName="app_user"/>
</changeSet>
</databaseChangeLog>